{
  "name": "estat",
  "version": "0.1.1",
  "description": "Easy statistics",
  "keywords": [
    "statistics",
    "stat",
    "Counter",
    "Gauge",
    "Histogram",
    "Meter"
  ],
  "author": "YourtionGuo <yourtion@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "typing": "dist/index.d.ts",
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "scripts": {
    "pretest": "npm run format && tsc",
    "tag": "git tag v`node -p 'require(\"./package\").version'`",
