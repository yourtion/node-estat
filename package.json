    "clean": "rm -rf dist",
    "compile": "tsc",
    "prepublishOnly": "npm run clean && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourtion/node-estat.git"
  },
  "bugs": {
    "url": "https://github.com/yourtion/node-estat/issues"
  },
  "homepage": "https://github.com/yourtion/node-estat#readme",
  "devDependencies": {
    "typescript": "^3.3.3333"
  }
}
